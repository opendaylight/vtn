##
## Start the VTN
##

IFS="
 	"
PATH=%PATH_SCRIPT%
export PATH
umask 022

DB_STOP=%INST_SBINDIR%/db_stop
UNCD_STOP=%INST_SBINDIR%/uncd_stop

# Terminate the UNC daemon.
$UNCD_STOP

# Terminate database
$DB_STOP
