##
## Start the VTN
##

IFS="
 	"
PATH=%PATH_SCRIPT%
export PATH
umask 022

DB_START=%INST_SBINDIR%/db_start
UNCD_START=%INST_SBINDIR%/uncd_start
UNC_DMCTL=%INST_BINDIR%/unc_dmctl

# Start database
$DB_START
status=$?
if [ $status -ne 0 ]; then
    exit $status
fi

# Start the UNC daemon.
$UNCD_START
status=$?
if [ $status -ne 0 ]; then
    exit $status
fi

# Activate the UNC daemon.
$UNC_DMCTL clevent ACT
